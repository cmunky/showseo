{
  "title": "showseo",
  "name": "showseo",
  "version": "1.0.0",
  "description": "Cross Browser Extension",
  "license": "GPL-2.0",
  "author": "cmunky",
  "main": "background-mozilla.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmunky/showseo.git"
  },
  "bugs": {
    "url": "https://github.com/cmunky/showseo/issues"
  },
  "homepage": "https://github.com/cmunky/showseo#readme",
  "devDependencies": {
    "bootstrap": "^3.3.6",
    "grunt": "~0.4.5",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-http": "^1.6.0",
    "grunt-jquery-builder": "^0.1.1",
    "grunt-jsbeautifier": "^0.2.10"    
  },
  "engines": {
    "node": ">= 0.10.0",
    "firefox": ">=38.0a1",
    "fennec": ">=38.0a1"
  },
  "readme": "Browser Plugin for extracting page markup details",
  "plugin_config": {
      "mozilla": {
          "script": [
              "events.js",
              "page-firefox.js"
          ]
      },
      "webkit": {
          "pkg": [
              "name",
              "version",
              "description"
          ],
          "manifest_version": 2,
          "permissions": [
              "tabs",
              "alarms",
              "storage"
          ],
          "background": {
              "scripts": [
                  "./js/lib/jquery.min.js",
                  "background-webkit.js"
              ]
          },
          "web_accessible_resources": [
              "data/template.html",
              "data/config-webkit.json",
              "js/result-chrome.js",
              "img/*.png",
              "css/*.css"
          ],
          "script": [
              "page-chrome.js"
          ]
      },
      "shared": {
          "target": [
              "http://gardens.wintermute/*"
          ],
          "style": [],
          "script": [
              "jquery.min.js"
          ],
          "options": {
            "option": "value"
          }
      }
  }
}
